  "license" : "ISC",
  "dependencies" : {
	"apollo-server-express" : "^2.9.7",
	"compression" : "^1.7.4",
	"cors" : "^2.8.5",
	"express" : "^4.17.1",
	"graphql" : "^14.5.8",
	"graphql-import-node" : "0.0.4",
	"graphql-playground-middleware-express" : "^1.7.12",
	"graphql-tools" : "^4.0.6",
	"http" : "0.0.0",
	"lodash" : "^4.17.15",
	"ncp" : "^2.0.0",
	"typescript" : "^3.6.4"
  },
  "devDependencies" : {
	"@types/compression" : "^1.0.1",
	"@types/cors" : "^2.8.6",
	"@types/express" : "^4.17.1",
	"@types/graphql" : "^14.5.0",
	"@types/lodash" : "^4.14.144",
	"@types/node" : "^12.11.7"
